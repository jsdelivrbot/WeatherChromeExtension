{
  "manifest_version": 2,

  "content_security_policy": "script-src 'self' https://maps.googleapis.com/maps/api/js; object-src 'self'",

  "name": "Weather-Beta",
  "description": "Choose city to check weather diagram for 5 days!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab"
  ]
}